#                replace mi:hasRange with owl:ObjectProperty.
#################################################################

mi:scheme-range a skos:ConceptScheme ;
  skos:prefLabel   "Register / range"@en ;
  skos:altLabel    "Registro"@es ;
  skos:definition  "Classifies instruments by their primary sounding register. Flat — no hierarchy among values."@en ;
  skos:hasTopConcept
    mi_range:soprano ,
    mi_range:mezzo-soprano ,
    mi_range:alto ,
    mi_range:tenor ,
    mi_range:bass ,
    mi_range:wide-range ,
    mi_range:unpitched
  .

mi_range:soprano a skos:Concept ;
  skos:prefLabel    "Soprano"@en ;
  skos:altLabel     "Soprano"@es ;
  skos:definition   "Primary sounding range in the soprano register (approximately C5 and above)."@en ;
  skos:topConceptOf mi:scheme-range ;
  skos:inScheme     mi:scheme-range .

mi_range:mezzo-soprano a skos:Concept ;
  skos:prefLabel    "Mezzo-soprano"@en ;
  skos:altLabel     "Mezzo-soprano"@es ;
  skos:definition   "Primary sounding range in the mezzo-soprano register (approximately A4–C6)."@en ;
  skos:topConceptOf mi:scheme-range ;
  skos:inScheme     mi:scheme-range .

mi_range:alto a skos:Concept ;
  skos:prefLabel    "Alto"@en ;
  skos:altLabel     "Alto"@es ;
  skos:definition   "Primary sounding range in the alto register (approximately E3–F5)."@en ;
  skos:topConceptOf mi:scheme-range ;
  skos:inScheme     mi:scheme-range .

mi_range:tenor a skos:Concept ;
  skos:prefLabel    "Tenor"@en ;
  skos:altLabel     "Tenor"@es ;
  skos:definition   "Primary sounding range in the tenor register (approximately C2–C5)."@en ;
  skos:topConceptOf mi:scheme-range ;
  skos:inScheme     mi:scheme-range .

mi_range:bass a skos:Concept ;
  skos:prefLabel    "Bass"@en ;
  skos:altLabel     "Bajo"@es ;
  skos:definition   "Primary sounding range in the bass register (approximately C1–E3)."@en ;
  skos:topConceptOf mi:scheme-range ;
  skos:inScheme     mi:scheme-range .

mi_range:wide-range a skos:Concept ;
  skos:prefLabel    "Wide range"@en ;
  skos:altLabel     "Rango amplio"@es ;
  skos:definition   "Instruments spanning three or more octaves, not reducible to a single register."@en ;
  skos:topConceptOf mi:scheme-range ;
