mi_range:unpitched a skos:Concept ;
  skos:prefLabel    "Unpitched"@en ;
  skos:altLabel     "Sin altura definida"@es ;
  skos:definition   "Instruments producing sound of indefinite or non-discrete pitch."@en ;
  skos:topConceptOf mi:scheme-range ;
  skos:inScheme     mi:scheme-range .


#################################################################
# SCHEME 3 — PERIOD
# Flat — historical periods are ordered chronologically but that
# order carries no SKOS-semantic weight; no skos:broader used.
# OWL migration: owl:NamedIndividual + owl:ObjectProperty mi:hasPeriod.
#################################################################

mi:scheme-period a skos:ConceptScheme ;
  skos:prefLabel   "Historical period"@en ;
  skos:altLabel    "Período histórico"@es ;
  skos:definition  "Classifies instruments by the historical period of their canonical form's emergence. Flat — no hierarchy among periods."@en ;
  skos:hasTopConcept
    mi_period:ancient ,
    mi_period:medieval ,
    mi_period:renaissance ,
    mi_period:baroque ,
    mi_period:classical-era ,
    mi_period:romantic ,
    mi_period:modern ,
    mi_period:contemporary
  .

mi_period:ancient a skos:Concept ;
  skos:prefLabel    "Ancient"@en ;
  skos:altLabel     "Antiguo"@es ;
  skos:definition   "Instruments whose canonical form originated in antiquity (before c. 500 CE)."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:medieval a skos:Concept ;
  skos:prefLabel    "Medieval"@en ;
  skos:altLabel     "Medieval"@es ;
  skos:definition   "Instruments whose canonical form emerged during the medieval period (c. 500–1400)."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:renaissance a skos:Concept ;
  skos:prefLabel    "Renaissance"@en ;
  skos:altLabel     "Renacimiento"@es ;
  skos:definition   "Instruments whose canonical form emerged or was codified during the Renaissance (c. 1400–1600)."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:baroque a skos:Concept ;
  skos:prefLabel    "Baroque"@en ;
  skos:altLabel     "Barroco"@es ;
  skos:definition   "Instruments whose canonical form emerged or was primary during the Baroque era (c. 1600–1750)."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:classical-era a skos:Concept ;
  skos:prefLabel    "Classical era"@en ;
  skos:altLabel     "Período clásico"@es ;
  skos:definition   "Instruments whose canonical form was established during the Classical era (c. 1750–1820)."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:romantic a skos:Concept ;
  skos:prefLabel    "Romantic"@en ;
  skos:altLabel     "Romántico"@es ;
  skos:definition   "Instruments whose canonical or modern form was established during the Romantic era (c. 1820–1900)."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:modern a skos:Concept ;
  skos:prefLabel    "Modern"@en ;
  skos:altLabel     "Moderno"@es ;
  skos:definition   "Instruments whose canonical form or widespread adoption occurred roughly 1900–1945."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .

mi_period:contemporary a skos:Concept ;
  skos:prefLabel    "Contemporary"@en ;
  skos:altLabel     "Contemporáneo"@es ;
  skos:definition   "Instruments invented or whose canonical form became established after 1945."@en ;
  skos:topConceptOf mi:scheme-period ;
  skos:inScheme     mi:scheme-period .


